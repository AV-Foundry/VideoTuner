profiles:
  - name: "Film - Transparent"
    description: "Live action film"
    settings:
      preset: "slow"
      no-open-gop: true
      bframes: 8
      rc-lookahead: 80
      tu-intra-depth: 3
      tu-inter-depth: 3
      subme: 5
      deblock: "-3:-3"
      no-sao: true
      selective-sao: 0
      aq-strength: 0.85
      aq-bias-strength: 1.00
      psy-rd: 2.10
      psy-rdoq: 1.05
      vbv-maxrate: 160000
      vbv-bufsize: 160000
      no-cutree: true
      weightb: true
      # Conditional parameters (differ by format)
      aq-mode:
        hdr: 4
        sdr: 5
